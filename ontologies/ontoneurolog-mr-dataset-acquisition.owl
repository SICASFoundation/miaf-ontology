    <!-- http://neurolog.unice.fr/ontoneurolog/v3.0/ontoneurolog-mr-dataset-acquisition.owl#refocused-gradient-echo-sequence -->

    <owl:Class rdf:about="http://neurolog.unice.fr/ontoneurolog/v3.0/ontoneurolog-mr-dataset-acquisition.owl#refocused-gradient-echo-sequence">
        <rdfs:subClassOf rdf:resource="http://neurolog.unice.fr/ontoneurolog/v3.0/ontoneurolog-mr-dataset-acquisition.owl#single-echo-gradient-echo-sequence"/>
        <skos:definition>Refocused GRE sequences use a refocusing gradient in the phase encoding direction to maximize (refocus) remaining xy-magnetization at the time when the next excitation is due, while the other two gradients are, in any case, balanced. In refocused gradient-echo sequences, the NMR signal comes not only from the nuclei within the slice, but also from spins that are downstream and outside the slice but still inside the receiver coil.</skos:definition>
    </owl:Class>
    


